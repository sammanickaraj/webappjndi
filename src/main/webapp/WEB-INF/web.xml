<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>GetIndexData</servlet-name>
  	<display-name>GetIndexData</display-name>
  	<description></description>
  	<servlet-class>com.hensation.webapp.GetIndexData</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetDBDetail</servlet-name>
  	<display-name>GetDBDetail</display-name>
  	<description></description>
  	<servlet-class>com.hensation.selectdb.GetDBDetail</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>GetIndexData</servlet-name>
  	<url-pattern>GetIndexData</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetDBDetail</servlet-name>
  	<url-pattern>/GetDBDetail</url-pattern>
  </servlet-mapping>
  <welcome-file-list>  
   <welcome-file>Index.html</welcome-file>  
  </welcome-file-list>  
</web-app>